    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.1",
    "babel-plugin-lodash": "^2.0.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.16.0",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-config-zanata": "^1.0.3",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-no-console-log": "^1.0.0",
    "eslint-plugin-react": "^3.16.1",
    "eslint-plugin-standard": "^1.3.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "jest-cli": "^0.4.18",
    "style-loader": "^0.12.3",
    "webpack": "^1.12.8",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-dev-server": "^1.10.1"
  },
  "dependencies": {
    "chart.js": "git://github.com/huangp/Chart.js.git",
    "classnames": "^2.1.3",
    "defined": "^1.0.0",
    "es6-promise": "^2.0.1",
    "events": "^1.0.2",
    "fixed-data-table": "^0.5.0",
    "flat": "^1.6.0",
    "flux": "^2.0.3",
    "keymirror": "^0.1.1",
    "lodash": "^4.0.1",
    "moment": "^2.10.6",
    "moment-range": "^1.2.0",
    "object-assign": "^2.0.0",
    "react": "^0.14.7",
    "react-addons-pure-render-mixin": "^0.14.6",
    "react-chartjs": "^0.4.0",
    "react-dom": "^0.14.6",
    "react-router": "^0.13.3",
    "superagent": "^0.21.0",
    "zanata-ui": "^2.0.0-2"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "./node_modules/react",
      "./node_modules/object-assign",
      "./node_modules/es6-promise",
      "./node_modules/events",
      "./src"
    ],
    "scriptPreprocessor": "./preprocessor.js",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  }
}
